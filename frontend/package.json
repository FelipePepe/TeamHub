{
  "name": "frontend",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=2048' next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:auth": "node scripts/run-e2e-with-auth.mjs --workers=1",
    "e2e:check-auth": "node scripts/check-e2e-auth.mjs",
    "demo": "playwright test --config=playwright.demo.config.ts --headed",
    "demo:record": "playwright test --config=playwright.demo.config.ts",
    "explore": "playwright test e2e/explorer-bot --headed",
    "explore:headless": "playwright test e2e/explorer-bot",
    "explore:chaos": "playwright test e2e/explorer-bot -g \"Chaos\" --headed"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.38.0",
    "@sentry/react": "^10.38.0",
    "@tanstack/react-query": "^5.62.11",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
